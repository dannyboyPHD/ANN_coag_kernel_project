function [dz_plots] = search_dz(v_start,dz,n,domain_limits)

dz_plots = zeros()
for i = 1:n
%     disp(search_steps)
    dz_50 = scan_till_limit(v_start,dz,nonLin_limit,params);
    dz_lims(search_steps,:) = dz_50;
%     hold on
%     plot(dz_50(1), dz_50(2),'o')

    v_start = v_start + dz_50;

end

end


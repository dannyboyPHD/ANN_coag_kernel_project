function [scaled_out] = output_scaling(unscaled_out,min_max)
beta_min = min_max(1,1);
beta_max = min_max(2,1);

scaled_out(:,1) = -1+2*(log10(unscaled_out(:,1)) - log10(beta_min))/(log10(beta_max)-log10(beta_min));

m = min(unscaled_out(:,2))
% m = -0.8;

n = max(unscaled_out(:,2))
% n = 3*10^6

scaled_out(:,2) = -1+ 2*(unscaled_out(:,2) - m)/(n - m);
end

